# Makefile for YABU

# Comment out the following four lines if you don't have MemCheck or HierProf
#MEMCHECK = MemCheck.cc
#HEIRPROF = -DHierProf_PROFILE

#MEMCHECKLIB = MemCheck:o.MC
#HIERPROFLIB = HierProf:o.hp


LIBS = \
	C:o.stubs\
	$(MEMCHECKLIB)\
	$(HIERPROFLIB)\
	OSLib:o.oslib\
	C:AJWLib.o.AJWLib\
	C:Desk.o.Desk


INCLUDE = C:,OSLib:

#DEBUG = -g

CCFLAGS = -throwback -strict -depend !Depend -Wg $(DEBUG) -Otime -I$(INCLUDE) $(HEIRPROF) -fah
#LINKFLAGS = -d

CC = $(MEMCHECK) cc
LINK = Link


!RunImage: Main.o
	$(LINK) $(LINKFLAGS) -o $@ Main.o $(LIBS)

.c.o:
	$(CC) -c $(CCFLAGS) $< -o $@


